{
  "image": "jomof/cloze-dev-data-image:latest",
  "hostRequirements": {
    "cpus": 8,
    "storage": "2048mb"
  },
  "waitFor": "onCreateCommand",
  "remoteEnv": {
      "HISTFILE": "/workspaces/cloze-data/.bash_history"
  },
  "postAttachCommand": {
    "bash": "bash"
  },
  
  "customizations": {
    "vscode": {
      "extensions": [
        "arcanis.vscode-zipfs",
        "ms-python.python",
        "GitHub.copilot",
        "redhat.vscode-yaml"
      ]
    },
    "codespaces": {
      "openFiles": [

      ]
    }
  }
}
