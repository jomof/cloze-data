# .bazelrc
build --disk_cache=/workspaces/cloze-data/bazel-cache --remote_cache=https://storage.googleapis.com/cloze-data-bazel-cache
build --google_credentials=.secrets/bazel-cache-key.json
fetch --disk_cache=/workspaces/cloze-data/bazel-cache --remote_cache=https://storage.googleapis.com/cloze-data-bazel-cache
fetch --google_credentials=.secrets/bazel-cache-key.json
test --disk_cache=/workspaces/cloze-data/bazel-cache --remote_cache=https://storage.googleapis.com/cloze-data-bazel-cache
test --google_credentials=.secrets/bazel-cache-key.json
test --cache_test_results=auto